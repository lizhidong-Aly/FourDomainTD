<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/custom_game/custom_status_panel.css"	/>
	</styles>
	<scripts>
		<include src="file://{resources}/scripts/custom_game/custom_status_panel.js" />
	</scripts>
		<Panel  html="false">
			<Panel id="CustomTowerInfoPanel"  html="true">
				<Panel class="SubPanel" onmouseover="ShowDetailedInfo('a')" onmouseout="HideToolTips()" id="TowerDmgInfo">
					<Panel class="StatusPanel" id="Dmg" >
						<Label class="defultStyle" id="status_label_a"	text="#status_dmage"/>
						<Label class="defultStyle data" id="label_a_data" html="true" text="100"/>
					</Panel>
					<Panel class="StatusPanel" id="Spe" >
						<Label class="defultStyle" id="status_label_b"	text="#status_speed"/>
						<Label class="defultStyle data" id="label_b_data" html="true" text="100"/>
					</Panel>
					<Panel class="StatusPanel" id="Range" >
						<Label class="defultStyle" id="status_label_c"	text="#status_range"/>
						<Label class="defultStyle data" id="label_c_data" html="true" text="100"/>
					</Panel>
				</Panel>
				<Panel class="SubPanel" id="TowerCostInfo" onmouseover="ShowDetailedInfo('b')"  onmouseout="HideToolTips()">
					<Panel class="StatusPanel" id="TowalCost" >
						<Label class="defultStyle" text="#status_total_cost"/>
						<Label class="defultStyle data" id="cost_data" html="true" text="0"/>
					</Panel>
					<Panel class="StatusPanel" id="UpgradeCost" >
						<Label class="defultStyle" text="#status_up_cost"/>
						<Label class="defultStyle data" id="upcost_data" html="true" text="0"/>
					</Panel>
					<Panel class="StatusPanel" id="Attribute" >
						<Label class="defultStyle" text="#status_attribute"/>
						<Label class="defultStyle data" id="attribute_data" html="true" text="N"/>
					</Panel>
				</Panel>
			</Panel>
		</Panel>
</root>